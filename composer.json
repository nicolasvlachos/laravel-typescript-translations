{
    "name": "nvl/laravel-typescript-translations",
    "description": "Generate TypeScript type definitions from Laravel translation files",
    "keywords": [
        "laravel",
        "typescript",
        "translations",
        "i18n",
        "types",
        "localization"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Nicolas Vlachos",
            "email": "vlachos.ni@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/console": "^12.0",
        "illuminate/support": "^12.0",
        "illuminate/filesystem": "^12.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.0",
        "orchestra/testbench": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "NVL\\LaravelTypescriptTranslations\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NVL\\LaravelTypescriptTranslations\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "NVL\\LaravelTypescriptTranslations\\TranslationTypesServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}